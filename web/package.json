{
  "name": "react18-vite3-ts-antd4",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --mode development",
    "build:dev": "tsc && vite build --mode development",
    "build": "tsc && vite build --mode production",
    "build:release": "tsc && vite build --mode release",
    "preview": "vite preview",
    "eslint": "eslint --fix --ext .js,.ts,.tsx ./src",
    "lint:prettier": "prettier --write --loglevel warn \"src/**/*.{js,ts,json,tsx,css,less,scss,html,md}\"",
    "pre-check": "tsc && npx lint-staged",
    "stylelint": "stylelint --cache --fix \"**/*.{less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "commit": "./node_modules/cz-customizable/standalone.js"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "npm run eslint"
    ],
    "src/**/*.{css,less,scss}": [
      "npm run stylelint"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  },
  "dependencies": {
    "@ant-design/icons": "^4.8.0",
    "@reduxjs/toolkit": "^1.9.1",
    "antd": "^4.24.6",
    "axios": "^1.2.1",
    "buffer": "^6.0.3",
    "crypto-js": "^3.3.0",
    "driver.js": "^0.9.8",
    "echarts": "^5.4.1",
    "echarts-liquidfill": "^3.1.0",
    "i18next": "^22.4.6",
    "js-md5": "^0.7.3",
    "jsencrypt": "^3.3.2",
    "moment": "^2.29.4",
    "nprogress": "^0.2.0",
    "qs": "^6.11.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.1.1",
    "react-particle-line": "^0.1.9",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.4.3",
    "react-transition-group": "^4.4.5",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "screenfull": "^6.0.2",
    "socket.io": "^4.5.4",
    "socket.io-client": "^4.5.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "@types/react-redux": "^7.1.24",
    "@types/react-router-dom": "^5.3.3",
    "@types/redux-logger": "^3.0.9",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "@vitejs/plugin-react": "^2.2.0",
    "autoprefixer": "^10.4.13",
    "cross-env": "^7.0.3",
    "cz-customizable": "^7.0.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.2",
    "less": "^4.1.3",
    "lint-staged": "^13.0.4",
    "mockjs": "^1.1.0",
    "postcss": "^8.4.19",
    "postcss-less": "^6.0.0",
    "prettier": "^2.8.0",
    "rollup-plugin-visualizer": "^5.8.3",
    "stylelint": "^14.15.0",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-less": "^1.0.6",
    "stylelint-prettier": "^2.0.0",
    "typescript": "^4.6.4",
    "vite": "^3.2.3",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-imp": "^2.3.1",
    "vite-plugin-mock": "^2.9.6",
    "vite-plugin-svg-icons": "^2.0.1"
  }
}
